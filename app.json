{
  "expo": {
    "name": "Cidade Verde",
    "slug": "CidadeVerde",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"      
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android":{
      "package":"com.engenhos.cidadeverde",
      "permissions":["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "adaptiveIcon":{
        "backgroundColor":"#b9e0b4",
        "backgroundImage":"./assets/icon-bgimage.png",
        "foregroundImage":"./assets/icon.png"
      }
    }
  }
}